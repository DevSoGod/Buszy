spawn(function()
	while wait(.2) do
		game:GetService("CoreGui").xBuszySky.TextLabel.Text = ">"
		wait(.2)
		game:GetService("CoreGui").xBuszySky.TextLabel.Text = ">>"
		wait(.2)
		game:GetService("CoreGui").xBuszySky.TextLabel.Text = ">>>"
		wait(.2)
		game:GetService("CoreGui").xBuszySky.TextLabel.Text = ">>"
	end
end)
function number(num)
	local v = tostring(num)
	while true do
		v, k = string.gsub(v, "^(-?%d+)(%d%d%d)", '%1,%2')
		if k == 0 then
			break
		end
	end
	return v
end
spawn(function()
	while wait(.2) do
		pcall(function()
			game:GetService("CoreGui").xBuszySky.coin.Value.TextLabel.Text = tostring(number(game:GetService("Players").LocalPlayer.Data.Currency.Coins.Value))
			game:GetService("CoreGui").xBuszySky.coin.Value.Text = tostring(number(game:GetService("Players").LocalPlayer.Data.Currency.Coins.Value))
		end)
	end
end)
spawn(function()
	while wait(.2) do
		pcall(function()
			game:GetService("CoreGui").xBuszySky.bacon.Value.TextLabel.Text = tostring(number(game:GetService("Players").LocalPlayer.Data.Resources.Bacon.Value))
			game:GetService("CoreGui").xBuszySky.bacon.Value.Text = tostring(number(game:GetService("Players").LocalPlayer.Data.Resources.Bacon.Value))
		end)
	end
end)
spawn(function()
	while wait(.2) do
		pcall(function()
			game:GetService("CoreGui").xBuszySky.meat.Value.TextLabel.Text = tostring(number(game:GetService("Players").LocalPlayer.Data.Resources.Meat.Value))
			game:GetService("CoreGui").xBuszySky.meat.Value.Text = tostring(number(game:GetService("Players").LocalPlayer.Data.Resources.Meat.Value))
		end)
	end
end)
spawn(function()
	while wait(.2) do
		pcall(function()
			PlayersCount.Text = tostring(#game.Players:GetChildren())
		end)
		pcall(function()
			if game:GetService("Players").LocalPlayer.Data.Worlds.Prehistoric.Value == true then
				PlayersCount.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			else
				PlayersCount.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			end
		end)
	end
end)
